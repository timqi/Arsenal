{
  "languageserver": {
      "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true,
        "completeUnimported": true,
        "deepCompletion": true
      }
    }
  },

  "snippets.ultisnips.directories": [
        "UltiSnips"
  ],

  "python.jediEnabled": false,
  "python.linting.enabled": false
}
