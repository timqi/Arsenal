[alias]
    a = add
    b = branch
    ck = checkout
	st = status -sb
    p = pull
    ll = log --online
	last = log -1 HEAD --stat
    cm = commit -m
    d = diff
    dv = difftool -t vimdiff -y
    cl = config --global -l
    se = !git rev-list --all | xargs git grep -F
